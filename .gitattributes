/.gitattributes     export-ignore
/.gitignore         export-ignore
/.styleci.yml       export-ignore
/.travis.yml        export-ignore
/.scrutinizer.yml   export-ignore
/phpunit.xml.dist   export-ignore
/docs               export-ignore
/tests              export-ignore
/tmp                export-ignore
